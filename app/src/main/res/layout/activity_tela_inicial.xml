<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:fillViewport="true"
    >
<androidx.constraintlayout.widget.ConstraintLayout

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".TelaInicial">

    <include android:id="@+id/toolbar" layout="@layout/activity_tool_bar"/>

    <ImageButton
        android:id="@+id/conteinerComponents1"
        android:layout_width="158dp"
        android:layout_height="140dp"
        app:srcCompat="@drawable/ic_grafic"
        tools:srcCompat="@drawable/ic_grafic"
        android:background="@drawable/contener_components"
        android:layout_marginStart="16dp"
        android:layout_marginTop="116dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar"
        />

    <ImageButton
        android:id="@+id/conteinerComponents2"
        android:layout_width="158dp"
        android:layout_height="140dp"
        android:layout_marginTop="116dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/contener_components"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar"
        app:srcCompat="@drawable/ic_edit"
        tools:srcCompat="@drawable/ic_edit" />

    <ImageButton
        android:id="@+id/conteinerComponents3"
        android:layout_width="match_parent"
        android:layout_height="140dp"
        android:layout_marginEnd="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="112dp"
        android:background="@drawable/contener_components"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/conteinerComponents1"
        app:srcCompat="@drawable/ic_camara"
        tools:srcCompat="@drawable/ic_camara"
        />






</androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>